;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Spatial",
"class": "Actor",
"language": "GDScript",
"path": "res://script/actor.gd"
}, {
"base": "Node",
"class": "WarioListen",
"language": "GDScript",
"path": "res://script/wario_listen.gd"
} ]
_global_script_class_icons={
"Actor": "",
"WarioListen": ""
}

[application]

config/name="Talking Wario HD"
run/main_scene="res://main.tscn"
